{
    "name": "poe-trade-fetch",
    "version": "0.0.0-development",
    "description": "poe trade fetch",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "require": "./dist/cjs/index.js",
            "import": "./dist/esm/index.js",
            "default": "./dist/esm/index.js"
        },
        "./*": {
            "types": "./dist/types/*.d.ts",
            "require": "./dist/cjs/*.js",
            "import": "./dist/esm/*.js",
            "default": "./dist/esm/*.js"
        }
    },
    "typesVersions": {
        ">=4.2": {
            "*": [
                "./dist/types/*"
            ]
        }
    },
    "type": "module",
    "release": {
        "branches": [
            "master",
            "dev"
        ],
        "plugins": [
            "@semantic-release/commit-analyzer",
            "@semantic-release/release-notes-generator",
            "@semantic-release/changelog",
            "@semantic-release/npm",
            "@semantic-release/github"
        ]
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "npm run prebuild && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc -p tsconfig.types.json && node fixTypeModule.js",
        "lint": "npx @biomejs/biome lint .",
        "lint-fix": "npx @biomejs/biome lint . --write",
        "format": "npx @biomejs/biome format . --write",
        "test": "jest ./src",
        "pub": "npm run build && npm publish",
        "semantic-release": "semantic-release"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Letale-vc/poe-trade-fetch.git"
    },
    "keywords": [
        "Path of Exile",
        "poe",
        "fetch"
    ],
    "author": "Helpless",
    "licenses": [
        {
            "type": "MIT",
            "url": "https://www.opensource.org/licenses/mit-license.php"
        },
        {
            "type": "Apache-2.0",
            "url": "https://opensource.org/licenses/apache2.0.php"
        }
    ],
    "bugs": {
        "url": "https://github.com/Letale-vc/poe-trade-fetch/issues"
    },
    "homepage": "https://github.com/Letale-vc/poe-trade-fetch#readme",
    "dependencies": {
        "axios": "^1.8.1",
        "cheerio": "^1.0.0"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/commit-analyzer": "^13.0.1",
        "@semantic-release/github": "^11.0.1",
        "@semantic-release/npm": "^12.0.1",
        "@semantic-release/release-notes-generator": "^14.0.3",
        "@types/eslint": "^9.6.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.9",
        "axios-mock-adapter": "^2.1.0",
        "jest": "^29.7.0",
        "rimraf": "^6.0.1",
        "semantic-release": "^24.2.3",
        "ts-jest": "^29.2.6",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
