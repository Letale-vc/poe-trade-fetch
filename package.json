{
  "name": "poe-trade-fetch",
  "version": "0.0.0-development",
  "description": "poe trade fetch",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./index.d.ts",
  "exports": {
    "./*": {
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "types": "./dist/types/*.d.ts",
      "default": "./dist/esm/*.js"
    }
  },
  "typesVersions": {
    ">=4.2": {
      "*": [
        "./dist/types/*"
      ]
    }
  },
  "type": "module",
  "release": {
    "branches": [
      "master",
      "dev"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json && tsc -p tsconfig.types.json && node fixTypeModule.js",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "format": "prettier . --write",
    "test": "jest ./src",
    "pub": "npm run build && npm publish",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Letale-vc/poe-trade-fetch.git"
  },
  "keywords": [
    "Path of Exile",
    "poe",
    "fetch"
  ],
  "author": "Helpless",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    },
    {
      "type": "Apache-2.0",
      "url": "https://opensource.org/licenses/apache2.0.php"
    }
  ],
  "bugs": {
    "url": "https://github.com/Letale-vc/poe-trade-fetch/issues"
  },
  "homepage": "https://github.com/Letale-vc/poe-trade-fetch#readme",
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/github": "^9.2.6",
    "@semantic-release/npm": "^11.0.2",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/eslint": "^8.56.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.56.0",
    "http-proxy-agent": "^7.0.1",
    "https-proxy-agent": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "semantic-release": "^22.0.12",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
